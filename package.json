{
  "name": "portfolio",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "check": "astro check",
    "check:types": "astro check && tsc --noEmit --skipLibCheck",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.astro",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx,.astro --fix",
    "test": "echo 'No tests configured yet'",
    "preflight": "npm run check:types && npm run build",
    "preflight:verbose": "echo '🔍 Running type checks...' && npm run check:types && echo '🏗️  Building project...' && npm run build && echo '✅ All preflight checks passed!'",
    "preflight:fix": "npm run check:types && npm run build",
    "preflight:full": "npm run check:types && npm run lint && npm run test && npm run build",
    "serve": "http-server -p 8080",
    "css:purge": "node scripts/purgeCSS.js",
    "images:opt": "node scripts/img-opt.js",
    "lint:a11y": "pa11y-ci"
  },
  "dependencies": {
    "@astrojs/react": "^4.3.0",
    "@types/d3": "^7.4.3",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "astro": "^5.12.3",
    "d3": "^7.9.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@eslint/js": "^9.32.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.32.0",
    "eslint-plugin-astro": "^1.3.1",
    "gsap": "^3.13.0",
    "http-server": "^14.1.1",
    "lucide-react": "^0.526.0",
    "pa11y-ci": "^4.0.0",
    "purgecss": "^7.0.2",
    "sharp": "^0.34.3",
    "typescript": "^5.8.3"
  }
}
