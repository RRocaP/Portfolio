{
  "name": "portfolio",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "npm run extract:critical && astro build",
    "build:prod": "npm run extract:critical && astro build",
    "extract:critical": "node scripts/extract-critical-css.js",
    "test:critical": "node scripts/test-critical-css.js",
    "preview": "astro preview",
    "astro": "astro",
    "check": "astro check",
    "check:types": "astro check && tsc --noEmit --skipLibCheck",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.astro",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx,.astro --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:content": "chmod +x scripts/test-content.sh && ./scripts/test-content.sh",
    "preflight": "npm run test:content && npm run check:types && npm run build",
    "preflight:verbose": "echo '🔍 Running content validation...' && npm run test:content && echo '🔍 Running type checks...' && npm run check:types && echo '🏗️  Building project...' && npm run build && echo '✅ All preflight checks passed!'",
    "preflight:fix": "npm run test:content && npm run check:types && npm run build",
    "preflight:full": "npm run test:content && npm run check:types && npm run lint && npm run test && npm run build",
    "prepare": "husky",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "storybook:serve": "npm run build-storybook && npx http-server storybook-static -p 6007",
    "qa": "npm run build && npx http-server dist -p 8080 >/dev/null & sleep 2 && npx lhci autorun --collect.url=http://localhost:8080 --assert.assertions='categories:performance>0.90,categories:accessibility>0.95,categories:seo>0.95,metrics.largest-contentful-paint<=2500,metrics.interaction-to-next-paint<=200,metrics.cumulative-layout-shift<=0.1' && npx playwright test",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:webkit": "playwright test --project=webkit",
    "test:e2e:mobile": "playwright test --project=mobile-chrome --project=mobile-safari",
    "test:accessibility": "playwright test --project=accessibility",
    "test:visual": "playwright test --project=visual",
    "test:performance": "playwright test --project=performance",
    "test:responsive": "playwright test tests/e2e/responsive.spec.ts",
    "test:multilingual": "playwright test tests/e2e/multilingual.spec.ts",
    "install:playwright": "playwright install",
    "install:playwright:deps": "playwright install --with-deps",
    "test:summary": "node scripts/test-summary.js",
    "test:report": "playwright show-report"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mjs}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.astro": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css,yaml,yml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@astrojs/react": "^4.3.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "astro": "^5.12.3",
    "d3-array": "^3.2.4",
    "d3-axis": "^3.0.0",
    "d3-drag": "^3.0.0",
    "d3-force": "^3.0.0",
    "d3-format": "^3.1.0",
    "d3-scale": "^4.0.2",
    "d3-selection": "^3.0.0",
    "d3-shape": "^3.2.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@chromatic-com/storybook": "^4.1.0",
    "@eslint/js": "^9.32.0",
    "@playwright/test": "^1.54.2",
    "@storybook/addon-a11y": "^9.1.1",
    "@storybook/addon-docs": "^9.1.1",
    "@storybook/addon-onboarding": "^9.1.1",
    "@storybook/addon-vitest": "^9.1.1",
    "@storybook/react-vite": "^9.1.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node-fetch": "^2.6.12",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.32.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-storybook": "^9.1.1",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.4",
    "playwright": "^1.54.2",
    "@axe-core/playwright": "^4.10.2",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "storybook": "^9.1.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "vite-plugin-compression2": "^2.2.0",
    "vitest": "^3.2.4",
    "vitest-axe": "^0.1.0",
    "@lhci/cli": "^0.15.1"
  }
}
