diff --git a/scripts/img-opt.js b/scripts/img-opt.js
new file mode 100644
index 0000000000000000000000000000000000000000..71fe1bcd1f49903a5e9471783440530954a35a08
--- /dev/null
+++ b/scripts/img-opt.js
@@ -0,0 +1,17 @@
+import { readdir } from 'fs/promises';
+import sharp from 'sharp';
+import { join } from 'path';
+
+async function convert() {
+  const dir = 'public';
+  const files = await readdir(dir);
+  const images = files.filter(f => /\.(png|jpg|jpeg)$/i.test(f));
+  await Promise.all(images.map(async img => {
+    const base = join(dir, img.replace(/\.(png|jpe?g)$/i, ''));
+    const src = join(dir, img);
+    await sharp(src).toFile(`${base}.webp`);
+    await sharp(src).toFile(`${base}.avif`);
+  }));
+}
+
+convert().catch(err => { console.error(err); process.exit(1); });
