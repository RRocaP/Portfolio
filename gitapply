+  ci: {
+    collect: {
+      url: ['http://localhost:8080'],
+      startServerCommand: 'http-server -c-1',
+    },
+    assert: {
+      assertions: {
+        'largest-contentful-paint': ['error', {maxNumericValue: 2500}],
+        'cumulative-layout-shift': ['error', {maxNumericValue: 0.1}],
+        'script-size-budget': ['error', {maxNumericValue: 35000}],
+      },
+    },
+    upload: {target: 'temporary-public-storage'},
+  }
+};
diff --git a/scripts/purgeCSS.js b/scripts/purgeCSS.js
new file mode 100644
index 0000000000000000000000000000000000000000..4810ca58d8ee4b068b95cf03cdc7aee9c1064b7f
--- /dev/null
+++ b/scripts/purgeCSS.js
@@ -0,0 +1,17 @@
+import { PurgeCSS } from 'purgecss';
+import { minify } from 'csso';
+import { readFile, writeFile } from 'fs/promises';
+
+async function build() {
+  const purge = await new PurgeCSS().purge({
+    content: ['index.html', 'main.js'],
+    css: ['styles.css']
+  });
+  const result = purge[0].css;
+  const minified = minify(result).css;
+  await writeFile('styles.min.css', minified);
+  const gzipped = (await import('zlib')).gzipSync(minified).length;
+  console.log('minified size (gz):', gzipped, 'bytes');
+}
+
+build().catch(err => { console.error(err); process.exit(1); });
