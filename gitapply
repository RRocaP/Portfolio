+  --font-step--2: clamp(0.83rem, calc(0.82rem + 0.1vw), 0.85rem);
+  --font-step--1: clamp(0.9rem, calc(0.88rem + 0.2vw), 0.95rem);
+  --font-step-0: clamp(1rem, calc(0.98rem + 0.25vw), 1.1rem);
+  --font-step-1: clamp(1.25rem, calc(1.2rem + 0.5vw), 1.5rem);
+  --font-step-2: clamp(1.56rem, calc(1.5rem + 0.75vw), 1.9rem);
+  --font-step-3: clamp(1.95rem, calc(1.9rem + 1vw), 2.4rem);
+  --font-step-4: clamp(2.44rem, calc(2.4rem + 1.3vw), 3rem);
+  --font-step-5: clamp(3.05rem, calc(3rem + 1.6vw), 3.75rem);
+  color-scheme: light dark;
+  --bg: var(--roma-1);
+  --text: #111;
+  --accent: #b40101;
+  --link: #b40101;
+}
